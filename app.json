{
  "expo": {
    "name": "Turing",
    "slug": "turing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/turing-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.turing.app",
      "backgroundColor": "#09090b"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/turing-icon.png",
        "backgroundColor": "#09090b"
      },
      "package": "com.turing.app"
    },
    "extra": {
      "eas": {
        "projectId": "turing-app"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-splash-screen"
    ]
  }
}
